<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>Signin</servlet-name>
        <servlet-class>servltpck.Signin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Watchlist</servlet-name>
        <servlet-class>servltpck.Watchlist</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Validate_register</servlet-name>
        <servlet-class>servltpck.Validate_register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Add_to_watchlist</servlet-name>
        <servlet-class>servltpck.Add_to_watchlist</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Remove_from_watchlist</servlet-name>
        <servlet-class>servltpck.Remove_from_watchlist</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Signin</servlet-name>
        <url-pattern>/Signin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Watchlist</servlet-name>
        <url-pattern>/Watchlist</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Validate_register</servlet-name>
        <url-pattern>/Validate_register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Add_to_watchlist</servlet-name>
        <url-pattern>/Add_to_watchlist</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Remove_from_watchlist</servlet-name>
        <url-pattern>/Remove_from_watchlist</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
